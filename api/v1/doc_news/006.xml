                            <value>
                                <key>1</key>
                                <name>qualification</name>
                            </value>
                            <value>
                                <key>2</key>
                                <name>specialization</name>
                            </value>
                            <value>
                                <key>3</key>
                                <name>complete basic school</name>
                            </value>
                        </values>
		</attribute>
		<attribute>
			<variable>true</variable>
			<code>userType</code>
